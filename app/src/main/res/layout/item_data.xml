            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@id/item_data_icon"
            android:layout_toRightOf="@id/item_data_icon"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川从神奈川"
            android:textColor="@color/black"
            android:textSize="16sp"/>

        <TextView
            android:id="@+id/item_data_age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_data_name"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="7dp"
            android:layout_toEndOf="@id/item_data_icon"
            android:layout_toRightOf="@id/item_data_icon"
            android:ellipsize="end"
            android:maxEms="5"
            android:maxLines="1"
            android:text="520岁"
            android:textColor="@color/default_gray"
            android:textSize="14sp"/>

        <ImageView
            android:id="@+id/item_data_sex"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_below="@id/item_data_name"
            android:layout_marginLeft="7dp"
            android:layout_marginStart="7dp"
            android:layout_marginTop="7dp"
            android:layout_toEndOf="@id/item_data_age"
            android:layout_toRightOf="@id/item_data_age"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_data_age"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="7dp"
            android:layout_toEndOf="@id/item_data_icon"
            android:layout_toRightOf="@id/item_data_icon"
            android:orientation="horizontal">

            <View
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/v_phone_model"/>

            <TextView
                android:id="@+id/item_data_model"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_marginStart="2dp"
                android:layout_weight="1"
                android:ellipsize="end"
                android:maxEms="10"
                android:maxLines="1"
                android:text="洛基亚N95洛基亚N95洛基亚N95洛基亚N95洛基亚N95洛基亚N95"
                android:textColor="@color/dark_gray"
                android:textSize="12sp"/>

            <View
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginEnd="2dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="2dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/v_location"/>

            <TextView
                android:id="@+id/item_data_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="end"
                android:maxEms="10"
                android:maxLines="1"
                android:text="八重村八重村八重村八重村"
                android:textColor="@color/default_blue"
                android:textSize="12sp"/>
        </LinearLayout>


